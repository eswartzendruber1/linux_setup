alias svn_push="svn diff --diff-cmd /usr/bin/diff > ~/mypatch"
alias svn_pull="patch -p0 -i ~/mypatch"

alias find_dup_mod='egrep "^ *module|^ *entity" * -h -r | sed -e '"'"'s/\s*entity\s\+\([a-zA-Z0-9_]*\)\s\+.*/\1/g'"'"' | sed -e '"'"'s/\s*module\s\+\([a-zA-Z0-9_]*\).*/\1/g'"'"' | sort | uniq --count | egrep -v "^ *1 " | sort -rn'



